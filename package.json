{
	"name": "new_bp",
	"version": "1.0.0",
	"description": "",
	"main": "dist/server/index.js",
	"scripts": {
		"build:client": "NODE_ENV=production webpack --progress",
		"build:client:dev": "NODE_ENV=development webpack --watch --progress",
		"build:server": "NODE_ENV=production babel server/ -d dist/server/",
		"build:server:dev": "NODE_ENV=development babel -w server/ -d dist/server/",
		"run:server:dev": "nodemon --inspect --watch \"./server/**\" dist/server/index.js",
		"build": "npm run clean && npm-run-all --parallel build:client build:server",
		"clean:client": "rm -rf dist/client",
		"clean:server": "rm -rf dist/server",
		"clean": "rm -rf dist",
		"dev": "npm run clean && npm run build:server && npm-run-all --parallel build:server:dev build:client:dev run:server:dev",
		"start": "node dist/server/index.js",
		"linter": "eslint src --fix"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.1.0",
		"express": "^4.18.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^1.3.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.6.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.20.12",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.22.20",
		"@babel/preset-react": "^7.18.6",
		"babel-loader": "^9.1.2",
		"css-loader": "^3.0.0",
		"dotenv": "^16.3.1",
		"eslint": "8.44",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-import": "^2.27.4",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^2.7.2",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.3",
		"style-loader": "^3.3.4",
		"webpack": "^5.75.0",
		"webpack-bundle-analyzer": "^4.7.0",
		"webpack-cli": "^5.0.1",
		"webpack-node-externals": "^3.0.0"
	},
